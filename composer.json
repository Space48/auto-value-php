{
    "name": "space48/auto-value",
    "type": "lib",
    "description": "Generated immutable value classes for PHP7.1+",
    "keywords": [
        "value",
        "value objects",
        "ddd"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "Josh Di Fabio",
            "email": "joshdifabio@gmail.com"
        }
    ],
    "require": {
        "php": "~8.1",
        "roave/better-reflection": "*",
        "symfony/console": "*"
    },
    "require-dev": {
        "phpunit/phpunit": "^7.2"
    },
    "autoload": {
        "psr-4": {
            "AutoValue\\": "src"
        },
        "files": [ "src/functions.php" ]
    },
    "autoload-dev": {
        "psr-4": {
            "AutoValue\\Demo\\": ["demo/src", "demo/auto"]
        }
    },
    "bin": ["bin/auto"]
}
